{
  "xml_root": "SABANG_ORDER_LIST",
  "header_fields": [
    {
      "name": "SEND_COMPAYNY_ID",
      "description": "사방넷 로그인아이디",
      "required": true,
      "note": "사방넷 어드민 로그인 아이디입력",
      "section": "header"
    },
    {
      "name": "SEND_AUTH_KEY",
      "description": "사방넷에서 발급한 인증키",
      "required": true,
      "note": "사방넷에서 발급 받은 인증키(만약 받지 않았다면 사방넷에서 인증키 발급 요청)",
      "section": "header"
    },
    {
      "name": "SEND_DATE",
      "description": "전송일자",
      "required": true,
      "note": "YYYYMMDD",
      "section": "header"
    }
  ],
  "data_fields": [
    {
      "name": "ORD_ST_DATE",
      "description": "검색 시작일자",
      "required": true,
      "note": "사방넷에 수집된 주문의 수집일자 기준의 검색조건",
      "section": "data"
    },
    {
      "name": "ORD_ED_DATE",
      "description": "검색 마지막일자",
      "required": true,
      "note": "사방넷에 수집된 주문의 수집일자 기준의 검색조건",
      "section": "data"
    },
    {
      "name": "ORD_FIELD",
      "description": "출력필드 리스트",
      "required": true,
      "note": "출력할 필드를 |의 구분자로 입력합니다.필드리스트는 아래 Field List 표를 참고하여 작성합니다.",
      "section": "data"
    },
    {
      "name": "JUNG_CHK_YN2",
      "description": "정산대조확인여부",
      "required": false,
      "note": "Y:확인완료, N:미확인, NULL:전체",
      "section": "data"
    },
    {
      "name": "ORDER_ID",
      "description": "주문번호(쇼핑몰)",
      "required": false,
      "note": "쇼핑몰 주문번호를 입력합니다.",
      "section": "data"
    },
    {
      "name": "MALL_ID",
      "description": "쇼핑몰CODE",
      "required": false,
      "note": "쇼핑몰코드를 입력합니다. shop 또는 chop 로 시작하는 코드를 입력합니다.\n기본정보 >쇼핑몰관리(지원) 또는 기본정보 >쇼핑몰관리(일반) 메뉴에서 코드 확인 가능합니다.",
      "section": "data"
    },
    {
      "name": "ORDER_STATUS",
      "description": "주문상태",
      "required": false,
      "note": "숫자로 입력합니다. 첫글자는 반드시 '(ENTER좌측Key)로 시작해야합니다.",
      "section": "data"
    },
    {
      "name": "LANG",
      "description": "encoding타입 선택",
      "required": false,
      "note": "UTF-8 타입으로 출력 원할 경우 입력합니다. UTF-8 외에 값이 들어오면 기본값 euc-kr로 출력됩니다.",
      "section": "data"
    },
    {
      "name": "PARTNER_ID",
      "description": "매입처ID",
      "required": false,
      "note": "매입처 아이디명을 입력합니다.",
      "section": "data"
    },
    {
      "name": "MALL_USER_ID",
      "description": "쇼핑몰ID",
      "required": false,
      "note": "해당 몰에 해당하는 쇼핑몰 아이디를 입력합니다. MALL_ID 입력시 사용가능합니다.",
      "section": "data"
    },
    {
      "name": "DPARTNER_ID",
      "description": "물류처ID",
      "required": false,
      "note": "물류처 아이디명을 입력합니다.",
      "section": "data"
    },
    {
      "name": "ACNT_REGS_SRNO",
      "description": "계정등록순번",
      "required": false,
      "note": "쇼핑몰로그인ID를 구분하는 고유 번호를 입력합니다.",
      "section": "data"
    }
  ],
  "authentication": {
    "SEND_COMPAYNY_ID": {
      "name": "SEND_COMPAYNY_ID",
      "description": "사방넷 로그인아이디",
      "required": true,
      "note": "사방넷 어드민 로그인 아이디입력",
      "section": "header"
    },
    "SEND_AUTH_KEY": {
      "name": "SEND_AUTH_KEY",
      "description": "사방넷에서 발급한 인증키",
      "required": true,
      "note": "사방넷에서 발급 받은 인증키(만약 받지 않았다면 사방넷에서 인증키 발급 요청)",
      "section": "header"
    }
  }
}