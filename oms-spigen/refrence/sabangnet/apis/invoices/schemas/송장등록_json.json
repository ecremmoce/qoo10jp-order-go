    },
    "takCode": {
      "type": "string",
      "description": "택배사코드",
      "xml_name": "TAK_CODE"
    },
    "takInvoice": {
      "type": "string",
      "description": "송장번호",
      "xml_name": "TAK_INVOICE"
    },
    "delvHopeDate": {
      "type": "string",
      "description": "배송희망일자",
      "xml_name": "DELV_HOPE_DATE"
    }
  }
}